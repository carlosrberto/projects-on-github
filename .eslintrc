{
  "extends": [
    "airbnb-base",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "import/no-extraneous-dependencies": [
      "error", {
        "devDependencies": [
          "setupTests.js",
          "scripts/**",
          "src/**/*.test.js"
        ]
      }
    ]
  },
  "settings": {
    "react": {
      "version": "16"
    }
  }
}
